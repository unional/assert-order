{
  "compilerOptions": {
    "declaration": true,
    "inlineSources": true,
    "lib": [
      "es2015"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "newLine": "LF",
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "typeRoots": ["typings"
    ],
    "types": [
        "core-js", "node"
        ]
    },
    "exclude":[
        "node_modules"]
  }
